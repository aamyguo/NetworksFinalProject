{
    "manifest_version": 2,
    "name": "Hide Me",
    "description": "Creates web traffic to hide your search",
    "version": "1.0",
    "icons": {"128: "icon_128.png"},
    "browser_action: {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}